{
  "name": "same-emoji",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en --port 4200 --proxy-config proxy.conf.json",
    "start:ja": "ng serve --configuration=ja --port 4300",
    "build": "npm run build:en && npm run build:ja",
    "build:en": "ng build --configuration=en",
    "build:ja": "ng build --configuration=ja",
    "test": "jest",
    "test:ci": "jest --ci --silent --runInBand",
    "test:watch": "jest --watch --onlyChanged",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "firebase deploy --only hosting",
    "build:deploy": "npm run build && npm run deploy",
    "format:ts": "prettier --config .prettierrc --write 'src/**/*.ts'",
    "format:md": "prettier --parser markdown --write '**/*.md'",
    "format:ts-import": "import-sort --write 'src/**/*.ts'",
    "format:css": "gulp csscomb",
    "format": "npm run format:ts && npm run format:md && npm run format:ts-import && npm run format:css"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
  },
  "dependencies": {
    "@angular/animations": "^7.1.2",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^7.1.2",
    "@angular/compiler": "^7.1.2",
    "@angular/core": "^7.1.2",
    "@angular/forms": "^7.1.2",
    "@angular/http": "^7.1.2",
    "@angular/material": "^7.1.1",
    "@angular/platform-browser": "^7.1.2",
    "@angular/platform-browser-dynamic": "^7.1.2",
    "@angular/router": "^7.1.2",
    "@types/moment": "^2.13.0",
    "core-js": "^2.5.4",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-emoji": "^1.8.1",
    "rxjs": "^6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.1.2",
    "@angular/language-service": "^7.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.109",
    "@types/node": "~8.9.4",
    "@types/node-emoji": "^1.8.0",
    "codelyzer": "^4.5.0",
    "csscomb": "^4.2.0",
    "gulp": "^3.9.1",
    "import-sort": "^5.2.0",
    "import-sort-cli": "^5.2.0",
    "import-sort-parser-typescript": "^5.0.0",
    "import-sort-style-module-alias": "^1.0.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^23.5.0",
    "jest-preset-angular": "^6.0.0",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "prettier": "^1.14.2",
    "protractor": "^5.4.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6"
  }
}
